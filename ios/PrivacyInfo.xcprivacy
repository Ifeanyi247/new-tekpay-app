<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>NSPrivacyCollectedDataTypes</key>
    <array>
        <!-- Connectivity Plus: May collect network-related info -->
        <dict>
            <key>NSPrivacyCollectedDataType</key>
            <string>NSPrivacyCollectedDataTypeLocation</string>
            <key>NSPrivacyCollectedDataUsage</key>
            <string>NSPrivacyCollectedDataUsageAppFunctionality</string>
        </dict>
        
        <!-- Share Plus: Handles sharing data but does not collect user data -->
        <dict>
            <key>NSPrivacyCollectedDataType</key>
            <string>NSPrivacyCollectedDataTypeIdentifiers</string>
            <key>NSPrivacyCollectedDataUsage</key>
            <string>NSPrivacyCollectedDataUsageAppFunctionality</string>
        </dict>
    </array>
</dict>
</plist>
